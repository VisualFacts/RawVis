                                 constraintName="fk_dataset_lat_id"
                                 referencedColumnNames="id"
                                 referencedTableName="field"/>

        <addForeignKeyConstraint baseColumnNames="lon_id"
                                 baseTableName="dataset"
                                 constraintName="fk_dataset_lon_id"
                                 referencedColumnNames="id"
                                 referencedTableName="field"/>

    </changeSet>
</databaseChangeLog>
